{
  "name": "michaelbutler/php-docker-project",
  "description": "Boilerplate php & docker web app",
  "type": "application",
  "homepage": "https://github.com/michaelbutler/php-docker-project",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Butler",
      "homepage": "https://butlerpc.net/"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-curl": "*",
    "ext-redis": "*",
    "ext-dom": "*",
    "slim/slim": "4.*",
    "slim/psr7": "^1.5",
    "psr/container": "^1.0",
    "vlucas/phpdotenv": "^5.4",
    "monolog/monolog": "^2.3",
    "php-di/php-di": "^6.3",
    "symfony/console": "^6.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "friendsofphp/php-cs-fixer": "^3.7",
    "vimeo/psalm": "^4.22"
  },
  "autoload": {
    "psr-4": {
      "MyApp\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  }
}
